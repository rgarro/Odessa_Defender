import com.avVenta.events.*;class com.avVenta.events.BroadcastableMovieClip extends MovieClip implements IBroadcastable{	private var broadcast:EventBroadcaster;			public function BroadcastableMovieClip(){		broadcast = new EventBroadcaster();	}		/**	 *	 */	public function addEventListener(eventName:String, listener:Object, methodName:String):Void {		broadcast.addEventListener(eventName,listener,methodName);	}	/**	 *	 */	public function broadcastEvent(eventName:String, data:Object):Void {		broadcast.broadcastEvent(eventName, this, data);	}	/**	 *	 */	public function removeEventListener(eventName:String, listener:Object, methodName:String):Void {		broadcast.removeEventListener(eventName,listener,methodName);	}		}